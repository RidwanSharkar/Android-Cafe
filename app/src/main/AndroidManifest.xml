<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.softmeth.project5">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.DemoTwoActivities"
        tools:targetApi="31">

        <activity
            android:name=".AllOrdersActivity"
            android:exported="false" />
        <activity
            android:name=".CurrentOrderActivity"
            android:exported="false" />
        <activity
            android:name=".OrderSandwichActivity"
            android:exported="false" />
        <activity
            android:name=".OrderDonutActivity"
            android:exported="false" />
        <activity
            android:name=".OrderCoffeeActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>